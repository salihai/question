{
    "version": 2,
    "builds": [
      {
        "src": "app.py", 
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/(.*)", 
        "dest": "app.py" 
      }
    ],
    "env": {
      "FLASK_ENV": "production", 
      "OTHER_ENV_VARIABLE": "value"
    }
  }
  